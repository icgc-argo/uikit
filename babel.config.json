{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": true,
        "labelFormat": "Uikit-[local]"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-object-rest-spread",

    [
      "module-resolver",
      {
        "root": ["./dist"],
        "alias": {
          "src": "./dist"
        }
      }
    ]
  ]
}
