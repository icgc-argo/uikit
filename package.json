{
	"name": "@icgc-argo/uikit",
	"version": "3.0.1",
	"description": "ARGO UI component library",
	"type": "module",
	"module": "index.js",
	"types": "index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/icgc-argo/uikit"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsc && vite build --watch && npm run build::assets",
		"build": "tsc && vite build && npm run build::assets",
		"build::assets": "cp ./package.json ./dist/package.json && cp -r ./src/assets ./dist/assets && cp ./README.md ./dist/README.md",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"type-check": "tsc --noEmit",
		"test": "NODE_PATH=./ ts-mocha -p ./tsconfig.test.json ./testSetup.tsx src/testSetup.tsx src/**/*.test.tsx",
		"test:watch": "NODE_PATH=./ ts-mocha --watch -p ./tsconfig.test.json ./testSetup.tsx src/testSetup.tsx src/**/*.test.tsx",
		"clean": "rm -rf dist && mkdir dist",
		"declarations": "tsc --emitDeclarationOnly",
		"publish-uikit": "cd dist && npm publish --access public",
		"create-component": "plop --plopfile ./src/plopfile.js"
	},
	"peerDependencies": {
		"@emotion/react": "^11.11.0",
		"react": "18.x.x",
		"react-dom": "18.x.x"
	},
	"dependencies": {
		"@emotion/react": "^11.11.0",
		"@emotion/styled": "^11.11.0",
		"@tanstack/react-table": "^8.7.6",
		"clsx": "^1.0.4",
		"color": "^3.1.2",
		"date-fns": "^1.30.1",
		"element-resize-detector": "^1.1.15",
		"jsdom": "^14.0.0",
		"lodash": "^4.17.14",
		"prop-types": "^15.7.2",
		"react-grid-system": "^4.4.6",
		"react-tippy": "^1.4.0",
		"react-transition-group": "^4.3.0",
		"recompose": "^0.30.0",
		"url-join": "^4.0.0"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^7.6.8",
		"@storybook/addon-essentials": "^7.6.8",
		"@storybook/addon-interactions": "^7.6.8",
		"@storybook/addon-knobs": "^7.0.2",
		"@storybook/addon-links": "^7.6.8",
		"@storybook/addon-onboarding": "^1.0.10",
		"@storybook/blocks": "^7.6.8",
		"@storybook/react": "^7.6.8",
		"@storybook/react-vite": "^7.6.8",
		"@storybook/test": "^7.6.8",
		"@storybook/testing-library": "^0.2.2",
		"@types/lodash": "^4.14.138",
		"@types/react": "^18.2.7",
		"@types/react-dom": "^17",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"eslint": "^8.55.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"eslint-plugin-storybook": "^0.6.15",
		"husky": "^2.2.0",
		"plop": "^2.3.0",
		"prettier": "^2.0.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"pretty-quick": "^2.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-json-view": "^1.21.3",
		"storybook": "^7.6.8",
		"typescript": "^5.3.3",
		"vite": "^5.0.8",
		"vite-plugin-dts": "^3.7.0"
	}
}
