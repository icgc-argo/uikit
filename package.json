{
  "name": "@icgc-argo/uikit",
  "version": "1.15.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/icgc-argo/uikit"
  },
  "scripts": {
    "storybook": "NODE_PATH=./ start-storybook -s ./ -p 6006",
    "build-storybook": "build-storybook",
    "create-component": "plop --plopfile ./src/plopfile.js",
    "type-check": "ttsc -p ./tsconfig.json",
    "clean-dist": "npx rimraf dist",
    "build:assets": "cp -r ./src/assets ./dist/assets",
    "build:ts": "ttsc",
    "build:babel": "babel ./dist --out-dir ./dist",
    "build:cleanup": "rm ./dist/**/*.jsx ./dist/*.jsx",
    "build:watch": "npm run clean-dist && mkdir dist && npm run build:assets && npm run build:ts -- --watch",
    "build": "npm run clean-dist && npm run build:ts && npm run build:assets && npm run build:cleanup",
    "publish-uikit": "cd dist && npm publish --access public",
    "postwatch": "npm run clean-dist",
    "test": "jest --config jest.config.ts",
    "test:watch": "npm run test -- --watch"
  },
  "peerDependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "react": "^16.8.6 || ^17.0.2",
    "react-dom": "^16.8.6 || ^17.0.2"
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "element-resize-detector": "^1.1.15",
    "lodash": "^4.17.14",
    "react-grid-system": "^4.4.6",
    "react-table": "6.10.0",
    "react-tippy": "^1.2.3",
    "react-transition-group": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.4.0",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@emotion/jest": "^11.8.0",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@storybook/addon-a11y": "^6.4.13",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-viewport": "^6.4.13",
    "@storybook/addons": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.5",
    "@types/expect": "^24.3.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^17.0.38",
    "@types/react-table": "^6.8.5",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-inline-svg": "^1.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "husky": "^2.2.0",
    "jest": "^27.5.1",
    "plop": "^2.3.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-json-view": "^1.21.3",
    "ts-jest": "^27.1.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.8",
    "tsc-watch": "^4.5.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  }
}
